{
  "manifest_version": 3,
  "name": "I HATE FAKE PNG",
  "description": "Browser extension that filters out non-transparent images from search results",
  "version": "1.0",
  "action": {
    "default_icon": "png.png"
  },
  "background": {
    "page": "out/background.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{a4bdc727-e704-417f-a7c6-3811fa5564cf}"
    }
  },  
  "options_ui": {
    "page": "out/options.html",
    "open_in_tab": false
  }
}
